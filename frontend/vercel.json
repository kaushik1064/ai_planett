{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "outputDirectory": "build",
  "env": {
    "VITE_BACKEND_BASE_URL": "https://ai-planett-c3s8.onrender.com"
  },
  "build": {
    "env": {
      "VITE_BACKEND_BASE_URL": "https://ai-planett-c3s8.onrender.com"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(css|js|svg|png|jpg|jpeg|gif|webp|ico|txt|map|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

